# environment variables for a Python development system in $HOME/usr

export SF=http://prdownloads.sourceforge.net

export QTDIR=$HOME/usr/lib/qt3.3
export QT4DIR=$HOME/usr/lib/qt4.3

export MANIPQT=$HOME/CVS/ManipQt

PATH=$HOME/usr/lib/qt4.3/bin:$PATH
PATH=$MANIPQT/bin:$PATH
PATH=$HOME/usr/bin:$PATH
PATH=$HOME/CVS/PyQwt/BUILD/gccxml-build/bin:$PATH
export PATH

LD_LIBRARY_PATH=$HOME/usr/lib/qt3.3/lib:$LD_LIBRARY_PATH
LD_LIBRARY_PATH=$HOME/usr/lib/qt4.3/lib:$LD_LIBRARY_PATH
LD_LIBRARY_PATH=$HOME/usr/lib:$LD_LIBRARY_PATH
export LD_LIBRARY_PATH

export PYTHONSTARTUP=$HOME/.pythonrc.py
export PYTHONDOCS=$HOME/usr/share/doc/python2.5/html
export PYTHONDOCDIR=$HOME/usr/share/doc/python2.5/html
export PYQT4DOCDIR=$HOME/usr/share/doc/PyQt4

export ENGAUGE_USERMANUAL=$HOME/CVS/PyQwt/BUILD/digitizer/usermanual

# Local Variables: ***
# mode: sh ***
# End: ***
