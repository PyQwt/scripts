# environment variables for a Python development system in $HOME/usr

export SF=http://belnet.dl.sourceforge.net/sourceforge
export SF=http://puzzle.dl.sourceforge.net/sourceforge

export QTDIR=$HOME/usr/lib/qt3.3

PATH=$HOME/usr/lib/qt4.2/bin:$PATH
PATH=$HOME/usr/bin:$PATH
export PATH

LD_LIBRARY_PATH=$HOME/usr/lib/qt3.3/lib:$LD_LIBRARY_PATH
LD_LIBRARY_PATH=$HOME/usr/lib/qt4.2/lib:$LD_LIBRARY_PATH
LD_LIBRARY_PATH=$HOME/usr/lib:$LD_LIBRARY_PATH
export LD_LIBRARY_PATH


# Local Variables: ***
# mode: sh ***
# End: ***
